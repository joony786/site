{
  "name": "site",
  "private": true,
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">=1.22.17"
  },
  "scripts": {
    "build": "next build",
    "clean": "rm -rf node_modules yarn lock; yarn cache clean",
    "deploy": "rm -rf node_modules/.cache && next build && next export -o out/hml && touch out/.nojekyll && git add out/ && git commit -m \"ci: deploy next.js to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "dev": "next dev",
    "export": "next export",
    "lint": "next lint",
    "lint-fix": "eslint --fix \"**/*.js\"",
    "prepare": "husky install",
    "start": "next start"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "emailjs-com": "^3.2.0",
    "formik": "^2.2.9",
    "imagemin-mozjpeg": "^10.0.0",
    "js-cookie": "^3.0.1",
    "next": "12.0.4",
    "next-optimized-images": "^2.6.2",
    "prop-types": "^15.8.0",
    "react": "17.0.2",
    "react-cookienotice": "^4.0.0",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "styled-components": "^5.3.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "babel-plugin-transform-define": "^1.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-next": "12.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4"
  }
}
